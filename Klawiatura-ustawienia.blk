    USEROPT_GUNNER_VIEW_ZOOM_SENS:i=100
    USEROPT_INVERTCAMERAY:i=0
    USEROPT_INVERTY_HELICOPTER:b=no
    USEROPT_INVERTY_HELICOPTER_GUNNER:b=no
    USEROPT_INVERTY_SHIP:b=no
    USEROPT_INVERTY_SPECTATOR:b=no
    USEROPT_INVERTY_SUBMARINE:b=no
    USEROPT_JOYFX:b=yes
    USEROPT_MOUSE_USAGE_NO_AIM:i=1
    USEROPT_SHIP_COMBINE_PRI_SEC_TRIGGERS:b=yes
    USEROPT_SINGLE_SHOT_BY_TURRET:b=yes
    USEROPT_WHEEL_CONTROL_SHIP:b=no
  }

  deviceMapping{
    joystick{
      connected:b=yes
      devId:t="x360"
      axesOffset:i=0
      name:t="Controller 1"
      buttonsOffset:i=0
      buttonsCount:i=28
      axesCount:i=7
    }

    joystick{
      connected:b=yes
      devId:t="054C:0CE6"
      axesOffset:i=7
      name:t="DualSense Wireless Controller"
      buttonsOffset:i=28
      buttonsCount:i=19
      axesCount:i=8
    }

    joystick{
      connected:b=no
      devId:t="146B:0D10"
      axesOffset:i=15
      name:t="Revolution Unlimited Pro Controller"
      buttonsOffset:i=47
      buttonsCount:i=18
      axesCount:i=8
    }
  }
